<header>
    {{> header}}
</header>
<main class="category">
    <h2>{{categorie}}</h2>
    <ul>
        {{#each data}}
        <li>
            <a href="/details/{{this.id}}">
                <img src="{{this.webImage.url}}1400" alt="{{this.title}}" loading="lazy">
                <h2>{{this.title}}</h2>
            </a>
        </li>
        {{/each}}
    </ul>
</main>