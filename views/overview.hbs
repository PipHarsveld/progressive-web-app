{{> header}}
<main class="overview">
    <h1>Overview</h1>
    <p>Ontdek onze meesterwerken vanuit je luie stoel!</p>
    <ul>
        {{#each data}}
        <li>
            <a href="/details/{{this.id}}">
                <img src="{{this.webImage.url}}" alt="{{this.title}}">
            </a>
        </li>
        {{/each}}
    </ul>
</main>